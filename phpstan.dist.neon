includes:
  - vendor/phpstan/phpstan-doctrine/extension.neon
  - vendor/phpstan/phpstan-phpunit/extension.neon
  - vendor/phpstan/phpstan-symfony/extension.neon
parameters:
  level: 8
  paths:
    - ./
  excludePaths:
    - ./bin/*
    - ./config/*
    - ./migrations/*
    - ./vendor/*
    - ./var/*
  symfony:
    container_xml_path: var/cache/dev/Kraska_OffBalance_KernelDevDebugContainer.xml
